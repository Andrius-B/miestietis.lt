<!-- Modal more -->
<div class="modal fade" id="profile-more" tabindex="-1" role="dialog" aria-labelledby="login-label" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                {% if  user  != null %}
                    <img src="{{ user.getProfilePicture() }}" alt="{{ user.getId()  }}" class = "profile-picture_medium">
                    <div>
                        <h4>
                            {{ user.getFirstName() }} {{ user.getLastName() }}
                            <a href="{{ path('fos_user_security_logout') }}">
                                <i class="fa fa-sign-out"></i>
                            </a>
                        </h4>
                        <p class="last-login">Paskutinis prisijungimas: {{ user.getLastLogin|date('Y-m-d H:i:s') }}</p>
                    </div>
                    <div class="counter-wrapper">
                        <div class="item-counter">
                            <span>28</span><br>
                            Sukurta <br>
                            problemų
                        </div>
                        <div class="item-counter">
                            <span>13</span><br>
                            Pritarta <br>
                            problemų
                        </div>
                    </div>
                {% endif %}
            </div>
            <hr>
            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-10 col-sm-offset-1">

                        <!-- isotope filters start -->
                        <div class="filters text-center filters-history">
                            <ul class="nav nav-pills nav-hist">
                                <li class="active"><a href="#" data-filter="*">Visi</a></li>
                                <li><a href="#" data-filter=".uploads">Įkėlimai</a></li>
                                <li><a href="#" data-filter=".activeInitiatives">Aktyvios iniciatyvos</a></li>
                            </ul>
                        </div>
                        <!-- isotope filters end -->
                        <div class="history-content">

                        </div>
                        <!-- list items start -->



                        {#there was div with history content class#}

                        {#<div class="text-center">#}
                            {#<a class="moreHistory" href="{{ path('ajax_history') }}"><i data-toggle="tooltip" data-placement="top" title="Peržiūrėti istoriją" class="fa fa-angle-double-down"></i></a>#}
                        {#</div>#}
                        <!-- list items end -->
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="text-center">
                    <a href="{{ path('fos_user_security_logout') }}"><input type="submit" value="Atsijungti" class="btn btn-default"></a>

                    <!-- here should add ajax request from database -->
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal end -->